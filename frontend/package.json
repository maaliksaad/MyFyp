{
  "name": "techbusters",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "mock": "node playwright/mocks/server.js",
    "generate": "graphql-codegen --config app/graphql/codegen.ts",
    "format:eslint": "eslint --ext .ts --fix",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format": "yarn run format:eslint && yarn run format:prettier",
    "lint:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:type": "tsc",
    "lint": "yarn run lint:lint && yarn run lint:type",
    "test": "vitest run",
    "test:cov": "yarn run test --coverage",
    "test:e2e": "npx playwright test"
  },
  "dependencies": {
    "@apollo/client": "3.9.6",
